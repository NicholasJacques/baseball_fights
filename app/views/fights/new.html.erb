<h1>Submit a new fight</h1>

<%= form_for @fight do |f| %>
  <%= f.label :embed_url %>
  <%= f.text_field :embed_url%>
  <br/>
  <%= f.label :date_occurred %>
  <%= f.date_field :date_occurred%>
  <br/>
  <%= f.label :description %>
  <%= f.text_area :description%>

  <%= f.submit "Submit" %>
<% end %>